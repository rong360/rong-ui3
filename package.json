{
  "name": "rong-ui3",
  "version": "0.0.1",
  "scripts": {
    "checked": "npm run generate:entry",
    "dev": "npm run checked && vite --mode dev",
    "build2": "run-p type-check build-only",
    "build:release": "rimraf ./release && npm run checked && vite build --config vite.config.build.ts && npm run generate:es && npm run generate:packagejson && npm run generate:resolver && npm run generate:es-style",
    "build:site": "npm run checked && vite build",
    "build:style": "vite build --config vite.config.build.css.ts",
    "build": "run-p build:release build:site",
    "preview": "vite preview",
    "generate:entry": "node build/generate-entry.js",
    "generate:packagejson": "node build/generate-packagejson.js",
    "generate:es": "vite build --config vite.config.build.disperse.ts",
    "generate:es-style": "gulp buildStyles",
    "generate:resolver": "vite build --config vite.config.build.resolver.ts && node build/generate-resolver.js",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@nutui/nutui": "^3.2.6",
    "less": "^4.1.3",
    "vant": "^4.3.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "codesandbox": "^2.2.3",
    "copy-to-clipboard": "^3.3.2",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "fs-extra": "^10.1.0",
    "glob": "^7.2.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-less-base-import": "^0.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "happy-dom": "^7.6.6",
    "he": "^1.2.0",
    "highlight.js": "^11.6.0",
    "jsdom": "^20.0.1",
    "markdown-it-container": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "sass": "^1.55.0",
    "typescript": "^4.9.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-md": "^0.20.4",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.6.4"
  }
}
